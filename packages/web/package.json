{
  "name": "bars",
  "version": "0.0.1",
  "description": "Bars.",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "lint": "npm-run-all lint:css lint:js",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint --ext .js src/",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test": "npm run test:unit && npm run test:feature",
    "test:cypress": "cypress run",
    "test:feature": "npm-run-all -p -r start test:cypress",
    "test:unit": "jest --config ./jest.config.js --coverage",
    "validate": "npm-run-all lint test"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/theSmaw/bars"
  },
  "maintainers": [
    {
      "name": "Ben Smawfield",
      "email": "bensmawfield@gmail.com"
    }
  ],
  "license": "UNLICENSED",
  "engines": {
    "npm": ">=4.0.0",
    "node": ">=6.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cypress": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "styled-components": "^3.2.6"
  }
}
